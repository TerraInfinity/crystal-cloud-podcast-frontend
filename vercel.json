{
    "rewrites": [{
            "source": "/api/(.*)",
            "destination": "/api/proxy"
        },
        {
            "source": "/(?!api)(.*)",
            "destination": "/index.html"
        }
    ],

    "headers": [{
        "source": "/assets/images/(.*)",
        "headers": [{
            "key": "Cache-Control",
            "value": "public, max-age=86400"
        }]
    }]

}