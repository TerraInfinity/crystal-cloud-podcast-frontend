{
    "rewrites": [{
            "source": "/api/:path*",
            "destination": "/api/proxy"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "headers": [{
        "source": "/assets/images/:path*",
        "headers": [{
            "key": "Cache-Control",
            "value": "public, max-age=86400"
        }]
    }]
}